FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/fluid";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    rightSide
    {
        type            zeroGradient;
    }
    leftSide
    {
        type            zeroGradient;
    }
    ground
    {
        type            zeroGradient;
    }
    upperWall
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
    inlet
    {
        type            zeroGradient;
    }
    fluid_to_solid
    {
        type            calculated;
        value           uniform 0;
    }
}


