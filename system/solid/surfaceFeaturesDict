FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      surfaceFeaturesDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

bod {
    surfaces ("simple-bod.stl");
    includedAngle    150;
    subsetFeatures {
        nonManifoldEdges     no;
        openEdges           yes;
        }
}

radiator {
    surfaces ("radiator.stl");
    includedAngle    150;
    subsetFeatures {
        nonManifoldEdges     no;
        openEdges           yes;
        }
}


